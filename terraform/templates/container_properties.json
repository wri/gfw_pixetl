{
  "command": [],
  "image": "${image_url}",
  "vcpus": ${cpu},
  "memory": ${memory},
  "environment": [
    {
      "name": "ENV",
      "value": "${environment}"
    },
    {
      "name": "JOB_ROLE_ARN",
      "value": "${clone_role_arn}"
    },
    {
      "name": "ECS_TASK_METADATA_RPS_LIMIT",
      "value": "100,150"
    }
  ],
  "jobRoleArn": "${job_role_arn}",
  "volumes": [
    {
      "host": {
        "sourcePath": "/tmp"
      },
      "name": "tmp"
    }
  ],
  "mountPoints": [
    {
      "sourceVolume": "tmp",
      "containerPath": "/tmp",
      "readOnly": false
    }
  ],
  "ulimits": [
    {
      "hardLimit": ${hardULimit},
      "name": "nofile",
      "softLimit": ${softULimit}
    }
  ],
  "linuxParameters": {
      "maxSwap": ${maxSwap},
      "swappiness": ${swappiness}
  }
}